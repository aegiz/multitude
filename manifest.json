{
	"manifest_version": 2,
	"name": "Multitude",
	"version": "0.1.0",
	"description": "Multitude Agency - Color Switcher demo extension",
	"content_scripts": [
		{
			"css": ["styles.css"],
			"js": ["content.js"],
			"matches": ["https://www.multitude.nl/en"],
			"run_at": "document_start"
		}
	],
	"icons": {
		"16": "icon/favicon-16x16.png",
		"32": "icon/favicon-32x32.png"
	},
	"browser_action": {
		"default_icon": "icon/favicon-32x32.png"
	}
}
